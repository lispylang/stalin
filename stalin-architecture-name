#!/bin/sh
case `uname -m`/`uname -s` in
  i[3456]86/*)
    echo "IA32";;
  x86_64/*)
    echo "AMD64";;
  sparc/Linux)
    echo "SPARC";;
  sparc64/Linux)
    echo "SPARC64";;
  sun4*/*)
    echo "SPARC";;
  IP*/IRIX*)
    echo "MIPS";;
  alpha/*)
    echo "Alpha";;
  arm64/Darwin)
    echo "ARM64";;
  aarch64/*)
    echo "ARM64";;
  arm*/*)
    echo "ARM";;
  m68k/Linux)
    echo "M68K";;
  "Power Macintosh/Darwin")
    echo "PowerPC";;
  s390/Linux)
    echo "S390";;
  000661704C00/AIX)
    echo "PowerPC64";;
  000661714C00/AIX)
    echo "PowerPC64";;
  000661724C00/AIX)
    echo "PowerPC64";;
  000661734C00/AIX)
    echo "PowerPC64";;
  *)
    echo "unknown";;
  esac
